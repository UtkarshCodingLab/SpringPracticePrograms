<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    
    <bean name="std1" class="com.setterinjectioncollection.Student">
    <property name="name" value="Suryauday" />
    
    <property name="Marks">
    <list>
    <value>53</value>
    <value>65</value>
    </list>
    </property>
    
    <property name="TeacherNames">
    <set>
    <value>Priya</value>
    <value>Pooja</value>
    </set>
    </property>
    
    <property name="course">
    <map>
    <entry key="101" value="Mathematics"></entry>
    <entry key="102" value="Science"></entry>
    </map>
    </property>
    
    </bean>

</beans>
