<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <!-- bean definitions here -->
    
    <context:annotation-config />
    
    <bean name="add1" class="com.springautowiring.annotationbased.Address">
    <constructor-arg value="Viman Nagar" />
    <constructor-arg value="Pune" />
    <constructor-arg value="India" />
    </bean>
    
     <bean name="add2" class="com.springautowiring.annotationbased.Address">
    <constructor-arg value="Gorwa" />
    <constructor-arg value="Vadodara" />
    <constructor-arg value="India" />
    </bean>
    
    <bean name="st" class="com.springautowiring.annotationbased.Student">
    <property name="roll_number" value="21" />
    <property name="name" value="Raju"></property>
    </bean>

</beans>